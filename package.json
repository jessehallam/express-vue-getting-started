{
    "name": "express-vue-getting-started",
    "description": "An express + vuejs getting started project.",
    "version": "0.0.1",
    "scripts": {
        "build-dev": "webpack --config webpack.dev.config.js",
        "build-dev-watch": "webpack --config webpack.dev.config.js -w",
        "dev": "concurrently --kill-others \"cross-env NODE_ENV=development node server/server.js\" \"cross-env NODE_ENV=development webpack -w --config webpack.dev.config.js\""
    },
    "dependencies": {
